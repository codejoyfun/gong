<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/base_background_color"
    android:orientation="vertical"
    android:paddingBottom="12dp"
    >
    <include layout="@layout/common_title_bar"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="vertical"
        >

        <RelativeLayout
            android:id="@+id/setItemLayout_1"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@drawable/white_btn_selector"
            android:layout_marginTop="12dp"
            >
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#e8eae7"
                />
            <ImageView
                android:id="@+id/setItemIcon_1"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="14dp"
                android:clickable="true"
                android:src="@drawable/set_up_modify_password"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="修改密码"
                android:textColor="#3e3e3e"
                android:textSize="@dimen/bottom_tab_font_size"
                android:layout_toRightOf="@id/setItemIcon_1"
                />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:src="@drawable/more_img"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#e8eae7"
                android:layout_alignParentBottom="true"
                />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/setItemLayout_2"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@drawable/white_btn_selector"
            android:layout_marginTop="12dp"
            >

            <ImageView
                android:id="@+id/setItemIcon_2"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="14dp"
                android:clickable="true"
                android:src="@drawable/set_up_current_version_number"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="当前版本"
                android:textColor="#3e3e3e"
                android:textSize="@dimen/bottom_tab_font_size"
                android:layout_toRightOf="@id/setItemIcon_2"
                />

            <ImageView
                android:id="@+id/rightRowIcon2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:src="@drawable/more_img"/>

            <TextView
                android:id="@+id/setItemName_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toLeftOf="@id/rightRowIcon2"
                android:layout_centerVertical="true"
                android:layout_marginLeft="4dp"
                android:text="1.0.0"
                android:textColor="#b3b3b3"
                android:layout_marginRight="2dp"
                android:textSize="14sp"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#e8eae7"
                android:layout_alignParentBottom="true"
                />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/setItemLayout_3"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@drawable/white_btn_selector"
            >
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#e8eae7"
                />
            <ImageView
                android:id="@+id/setItemIcon_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="14dp"
                android:clickable="true"
                android:src="@drawable/set_up_clears"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="清理缓存"
                android:textColor="#3e3e3e"
                android:textSize="@dimen/bottom_tab_font_size"
                android:layout_toRightOf="@id/setItemIcon_3"

                />

            <ImageView
                android:id="@+id/rightRowIcon1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:src="@drawable/more_img"/>

            <TextView
                android:id="@+id/setItemName_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toLeftOf="@id/rightRowIcon1"
                android:layout_centerVertical="true"
                android:layout_marginLeft="4dp"
                android:text="10M"
                android:textColor="#b3b3b3"
                android:layout_marginRight="2dp"
                android:textSize="14sp"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#e8eae7"
                android:layout_alignParentBottom="true"
                />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/setItemLayout_4"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@drawable/white_btn_selector"
            android:layout_marginTop="12dp"
            >
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#e8eae7"
                />
            <ImageView
                android:id="@+id/setItemIcon_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="14dp"
                android:clickable="true"
                android:src="@drawable/set_up_mark"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="给我评分"
                android:textColor="#3e3e3e"
                android:textSize="@dimen/bottom_tab_font_size"
                android:layout_toRightOf="@id/setItemIcon_4"
                />

            <ImageView
                android:id="@+id/rightRowIcon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:src="@drawable/more_img"/>

            <!--<TextView-->
                <!--android:layout_width="wrap_content"-->
                <!--android:layout_height="wrap_content"-->
                <!--android:layout_toLeftOf="@id/rightRowIcon"-->
                <!--android:layout_centerVertical="true"-->
                <!--android:layout_marginLeft="4dp"-->
                <!--android:text="10M"-->
                <!--android:textColor="#b3b3b3"-->
                <!--android:layout_marginRight="2dp"-->
                <!--android:textSize="14sp"/>-->
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#e8eae7"
                android:layout_alignParentBottom="true"
                />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/setItemLayout_5"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@drawable/white_btn_selector"
            >
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#e8eae7"
                />
            <ImageView
                android:id="@+id/setItemIcon_5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="14dp"
                android:clickable="true"
                android:src="@drawable/set_up_about"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:text="关于我们"
                android:textColor="#3e3e3e"
                android:textSize="@dimen/bottom_tab_font_size"
                android:layout_toRightOf="@id/setItemIcon_5"
                />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:src="@drawable/more_img"/>

            <!--<TextView-->
                <!--android:layout_width="wrap_content"-->
                <!--android:layout_height="wrap_content"-->
                <!--android:layout_toLeftOf="@id/rightRowIcon"-->
                <!--android:layout_centerVertical="true"-->
                <!--android:layout_marginLeft="4dp"-->
                <!--android:text="10M"-->
                <!--android:textColor="#b3b3b3"-->
                <!--android:layout_marginRight="2dp"-->
                <!--android:textSize="14sp"/>-->
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#e8eae7"
                android:layout_alignParentBottom="true"
                />
        </RelativeLayout>
        <TextView
            android:id="@+id/exit_user"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="22dp"
            android:layout_marginBottom="40dp"
            android:background="@drawable/white_btn_selector"
            android:text="退出登录"
            android:textColor="#fd5b67"
            android:textSize="16sp"
            android:visibility="visible"
            android:gravity="center"
            android:paddingTop="12dp"
            android:paddingBottom="12dp"
            />
    </LinearLayout>

</LinearLayout>